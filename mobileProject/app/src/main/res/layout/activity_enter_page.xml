<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="e.dell.mobileproject.EnterPage">

    <RelativeLayout
        android:layout_width="420dp"
        android:layout_height="605dp"
        android:background="@drawable/background"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView11"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="16dp"
            app:srcCompat="@drawable/logo" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="83dp"
            app:srcCompat="@drawable/sanat" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView9"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="34dp"
            android:text="JUEGO GAME"
            android:textAlignment="textStart"
            android:textColor="@android:color/background_dark"
            android:textSize="55sp"
            android:textStyle="bold|italic" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignEnd="@+id/buttonfacebook"
            android:layout_alignRight="@+id/buttonfacebook"
            android:layout_alignTop="@+id/imageView9"
            android:layout_marginTop="35dp"
            app:srcCompat="@drawable/spor" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_below="@+id/imageView10"
            android:layout_marginTop="57dp"
            android:layout_toLeftOf="@+id/imageView9"
            android:layout_toStartOf="@+id/imageView9"
            app:srcCompat="@drawable/eglence" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/imageView9"
            android:layout_alignStart="@+id/imageView9"
            android:layout_below="@+id/textView10"
            android:layout_marginTop="36dp"
            app:srcCompat="@drawable/bilim" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignTop="@+id/imageView10"
            android:layout_toEndOf="@+id/imageView9"
            android:layout_toRightOf="@+id/imageView9"
            app:srcCompat="@drawable/cografya" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignLeft="@+id/buttontwitter"
            android:layout_alignStart="@+id/buttontwitter"
            android:layout_below="@+id/textView10"
            app:srcCompat="@drawable/tarih" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/imageView12"
            android:layout_marginEnd="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="52dp"
            android:text="Have fun while learning..."
            android:textColor="@android:color/background_dark"
            android:textSize="24sp"
            android:textStyle="bold|italic" />

        <Button
            android:id="@+id/buttonfacebook"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBaseline="@+id/buttontwitter"
            android:layout_alignBottom="@+id/buttontwitter"
            android:layout_marginEnd="11dp"
            android:layout_marginRight="11dp"
            android:layout_toLeftOf="@+id/buttoninsta"
            android:layout_toStartOf="@+id/buttoninsta"
            android:background="@drawable/facebook" />

        <Button
            android:id="@+id/buttoninsta"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/textView11"
            android:layout_marginLeft="11dp"
            android:layout_marginStart="11dp"
            android:layout_marginTop="18dp"
            android:layout_toEndOf="@+id/imageView11"
            android:layout_toRightOf="@+id/imageView11"
            android:background="@drawable/instagram" />

        <Button
            android:id="@+id/buttontwitter"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBaseline="@+id/buttoninsta"
            android:layout_alignBottom="@+id/buttoninsta"
            android:layout_marginLeft="21dp"
            android:layout_marginStart="21dp"
            android:layout_toEndOf="@+id/buttoninsta"
            android:layout_toRightOf="@+id/buttoninsta"
            android:background="@drawable/twitter" />

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
